<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

<record id="view_helpdesk_category_form" model="ir.ui.view">
    <field name="model">helpdesk.ticket.category</field>
    <field name="inherit_id" ref="helpdesk_mgmt_rest_api.view_helpdesk_category_form" />
    <field name="arch" type="xml">
        <field name="is_available_rest_api" position="after">
            <field
                    name="automatically_generate_return_for"
                    attrs="{'invisible': [('is_available_rest_api', '=', False)]}"
                />
        </field>
    </field>
</record>

</odoo>
